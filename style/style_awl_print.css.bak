/* CSS Document */
/* Feuille de style pour Alienor Web Libre (version imprimable) */

/* Balise */
body {
	font-family: "Times New Roman", Times, serif;
	font-size: 0.8em;
	text-align: left;
	margin: 2px;
}

h1 {
	background-image: url(../images/ban-alienor.gif);
	background-repeat: no-repeat;
	background-position: center;
	color: #666666;
	height: 60px;
	width: 468px;
	text-align: center;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 150%;
}

input, select {
	font-family: "Times New Roman", Times, serif;
	font-size: 0.9em;
	margin: 2px;
}
table {
	font-family: "Times New Roman", Times, serif;
	font-size: 1em;
}

textarea {
	margin: 2px;
	font-size: 0.9em;
	font-family: "Times New Roman", Times, serif;
}

/* Navigation */

a:link {
	color: #333333;
	text-decoration: underline;
	text-align: left;
	font-size: 100%;
}

a:visited {
	color: #333333;
	text-decoration: underline;
}

a:hover {
	color: #FF0000;
	text-decoration: underline;
}

a:active {
	color: #FF0000;
	text-decoration: none;
}


